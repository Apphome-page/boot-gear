{
  "name": "bootgear",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "inspect": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "rimraf .next out && next build && next export",
    "deploy:prod": "cp netlify.prod.toml netlify.toml && cp .env.production .env.local && cp firebase.prod.json firebase.json && npm run export",
    "deploy:dev": "cp netlify.dev.toml netlify.toml && cp .env.development .env.local && firebase.dev.json firebase.json && npm run export",
    "fire-install": "cd ./functions && npm install",
    "fire-serve": "firebase functions:config:get > ./functions/.runtimeconfig.json && env-cmd -f ./.env.development firebase emulators:start --only functions",
    "fire-deploy": "firebase deploy --only functions"
  },
  "dependencies": {
    "@emotion-icons/bootstrap": "^2.11.0",
    "@emotion-icons/feather": "^2.10.0",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "bootstrap": "^4.5.2",
    "cross-fetch": "^3.0.6",
    "dot": "^1.1.3",
    "env-cmd": "^10.1.0",
    "file-saver": "^2.0.2",
    "firebase": "^7.23.0",
    "glob": "^7.1.6",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "next": "9.5.3",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pica": "^6.1.1",
    "rc-color-picker": "^1.2.6",
    "react": "16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "16.13.1",
    "react-firebaseui": "^4.1.0",
    "sass": "^1.27.0",
    "swr": "^0.3.8",
    "use-http": "^1.0.15"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2"
  }
}
